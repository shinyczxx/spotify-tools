{
  "themes": {
    "czxx-terminal": {
      "name": "CZXX Terminal",
      "description": "Retro terminal aesthetic with modern touches",
      "colors": {
        "primary": "#00ffff",
        "primaryBright": "#66ffff",
        "primaryDim": "#0099cc",
        "primaryDark": "#003366",
        "background": "#000000",
        "backgroundDark": "#0a0a0a",
        "backgroundMedium": "#1a1a1a",
        "backgroundLight": "#2a2a2a",
        "text": "#00ffff",
        "textDim": "#cccccc",
        "textMuted": "#666666",
        "success": "#00ff88",
        "warning": "#ffaa00",
        "error": "#ff4444",
        "accent": "#ff00ff"
      },
      "effects": {
        "glow": "0 0 1px var(--primary)",
        "glowBright": "0 0 2px var(--primary-bright)",
        "glowIntense": "0 0 4px var(--primary), 0 0 8px var(--primary-bright)",
        "textShadow": "0 0 1px var(--primary)",
        "boxShadow": "inset 0 0 10px rgba(0, 255, 255, 0.1)"
      },
      "typography": {
        "fontFamily": "'Fixedsys', 'Courier New', monospace",
        "fontSize": "14px",
        "lineHeight": "1.3",
        "letterSpacing": "1px",
        "textTransform": "lowercase"
      },
      "crt": {
        "enabled": true,
        "scanlineOpacity": 0.1,
        "glowIntensity": 0.8,
        "curvature": 0.02,
        "flicker": true
      }
    },
